    <t:Paragraph LineSpacing="1">
      <t:Span FontFamily="Verdana" FontStyle="Normal" FontWeight="Normal" ForeColor="#FF000000" HighlightColor="#00FFFFFF" Text="One of the new features of RadRichTextBox introduced in Q3 2011 is " UnderlineColor="#FF000000" UnderlineDecoration="None" />
      <t:Span FontFamily="Verdana" FontStyle="Normal" FontWeight="Bold" ForeColor="#FF000000" HighlightColor="#00FFFFFF" Text="Document Protection" UnderlineColor="#FF000000" UnderlineDecoration="None" />
