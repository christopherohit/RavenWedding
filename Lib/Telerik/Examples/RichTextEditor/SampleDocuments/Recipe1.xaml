            <custom1:RecipeRangeEnd AnnotationID="5" />
            <t:PermissionRangeEnd AnnotationID="6" />
        </t:Paragraph>
        <t:Paragraph LineSpacing="1">
            <t:Span Text="Ingredients:" />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="1" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:PermissionRangeStart AnnotationID="7">
                <t:PermissionRangeInfo Type="Everyone" />
            </t:PermissionRangeStart>
            <custom1:RecipeRangeStart AnnotationID="8" Name="ingredient" />
            <t:Span Text="5 oz.(140g.) any small pasta 6 oz.(170g.) provolone cheese, shredded" />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="1" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:Span Text="4 thin sliced tomatoes 10 oz (280g.) Italian dressing" />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="1" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:Span Text="1 small onion, chopped 1/2 red pepper" />
            <custom1:RecipeRangeEnd AnnotationID="8" />
            <t:PermissionRangeEnd AnnotationID="7" />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="24" LineSpacing="1" StyleName="ListParagraph">
            <t:Span Text="Instructions:" />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="2" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:PermissionRangeStart AnnotationID="9">
                <t:PermissionRangeInfo Type="Everyone" />
            </t:PermissionRangeStart>
            <custom1:RecipeRangeStart AnnotationID="10" Name="instructions" />
            <t:Span Text="Bring salted water to a boil. Add the pasta and cook until the pasta is al dente, or for 8-10 minutes. Drain the pasta, and rinse with cold water. Drain the pasta again." />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="2" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:Span Text="Mix the pasta with one third of the dressing before placing in the bottom of a large salad bowl. Add layers of tomato, onion, red pepper, garlic, olives, and provolone cheese on top of the pasta before adding in the rest of the dressing." />
        </t:Paragraph>
        <t:Paragraph FirstLineIndent="-24" LeftIndent="48" LineSpacing="1" StyleName="ListParagraph">
            <t:Paragraph.ListItemSerializationData>
                <t:ListItemSerializationData ListID="2" ListLevel="0" />
            </t:Paragraph.ListItemSerializationData>
            <t:Span Text="Sprinkle basil leaves, parsley, and parmesan cheese, and salt for seasoning on top of the salad." />
            <custom1:RecipeRangeEnd AnnotationID="10" />
            <t:PermissionRangeEnd AnnotationID="9" />
        </t:Paragraph>
        <t:Paragraph LineSpacing="1">
            <t:Span Text="Further details: " />
            <t:Span Text="Cover the salad and refrigerate for three hours before tossing the salad." />
        </t:Paragraph>
        <t:Paragraph LineSpacing="1">
            <t:PermissionRangeStart AnnotationID="11">
                <t:PermissionRangeInfo Type="Everyone" />
            </t:PermissionRangeStart>
