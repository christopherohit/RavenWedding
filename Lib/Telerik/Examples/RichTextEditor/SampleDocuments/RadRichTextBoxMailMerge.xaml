                        </t:FieldPropertySetter.NestedFieldValue>
                    </t:FieldPropertySetter>
                </t:IncludePictureField.FieldPropertySetters>
            </t:IncludePictureField>
        </t:FieldRangeStart>
        <t:Span FontFamily="Calibri" FontSize="13.33" ForeColor="#FF767676" Text="{}{INCLUDEPICTURE " />
        <t:FieldRangeStart AnnotationID="3588938">
