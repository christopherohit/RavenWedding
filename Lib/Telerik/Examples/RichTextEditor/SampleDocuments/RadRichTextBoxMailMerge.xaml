  <t:Section HeaderTopMargin="15" PageMargin="96,96,96,96">
   
    <t:Paragraph Background="#FF319B47" FontSize="13.3299999237061" LineSpacing="1" SpacingAfter="20">
      <t:Span FontFamily="Calibri" FontSize="13.3299999237061" ForeColor="#FFFFFFFF" Text="&#x9;Telerik Newsletter" />
    </t:Paragraph>
    <t:Paragraph Background="#00FFFFFF" LeftIndent="24" LineSpacing="1" SpacingAfter="2.66666674613953">
        <t:FieldRangeStart AnnotationID="32300447">
            <t:IncludePictureField DisplayMode="Code">
                <t:IncludePictureField.FieldPropertySetters>
                    <t:FieldPropertySetter PropertyName="ImageUri">
                        <t:FieldPropertySetter.NestedFieldValue>
