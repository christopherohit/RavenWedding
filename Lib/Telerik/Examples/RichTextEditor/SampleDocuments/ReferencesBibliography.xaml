            <t:Span Text="The Perspectives of Using Online Marketing Research" />
        </t:Paragraph>
        <t:Paragraph StyleName="Heading2" TextAlignment="Center" />
        <t:Paragraph StyleName="Heading2" TextAlignment="Center">
