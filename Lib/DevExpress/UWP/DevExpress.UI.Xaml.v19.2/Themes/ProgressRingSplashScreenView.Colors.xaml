<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewStateForeground" Color="#FF106EBE" Core:ColorKey.Id="ProgressRingSplashScreenViewColors.StateForeground.Light"/>
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewForeground" Color="#FF106EBE" Core:ColorKey.Id="ProgressRingSplashScreenViewColors.Foreground.Light"/>
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewBackground" Color="#FFFFFFFF" Core:ColorKey.Id="ProgressRingSplashScreenViewColors.Background.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewStateForeground" Color="#FFDCDCDC"  Core:ColorKey.Id="ProgressRingSplashScreenViewColors.StateForeground.Default" />
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewForeground" Color="#FFDCDCDC" Core:ColorKey.Id="ProgressRingSplashScreenViewColors.Foreground.Default" />
            <SolidColorBrush x:Key="ProgressRingSplashScreenViewBackground" Color="#C0000000" Core:ColorKey.Id="ProgressRingSplashScreenViewColors.Background.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>




