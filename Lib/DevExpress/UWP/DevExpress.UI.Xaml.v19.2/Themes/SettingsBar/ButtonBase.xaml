<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Internal="using:DevExpress.UI.Xaml.Controls.Internal.SettingsBar"
    xmlns:dx="using:DevExpress.Core">

    <dx:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <Internal:ClonePathGeometryConverter x:Key="GeometryCopyConverter"/>


    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <x:Double x:Key="SettingsBarItemDisabledOpacity">0.35</x:Double>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <x:Double x:Key="SettingsBarItemDisabledOpacity">0.25</x:Double>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>

