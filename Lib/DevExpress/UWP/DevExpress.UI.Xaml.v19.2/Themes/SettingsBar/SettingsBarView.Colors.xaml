<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="SettingsBarMoreButtonFlyoutBackgroundBrush" Color="#FF0F0F0F" Core:ColorKey.Id="SettingsBarColors.MoreButtonFlyoutBackground.Light"/>
            <SolidColorBrush x:Key="SettingsBarMoreButtonFlyoutBorderBrush" Color="#0FFFFFFF"  Core:ColorKey.Id="SettingsBarColors.MoreButtonFlyoutBorderBrush.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="SettingsBarMoreButtonFlyoutBackgroundBrush" Color="#FF0F0F0F"  Core:ColorKey.Id="SettingsBarColors.MoreButtonFlyoutBackground.Default" />
            <SolidColorBrush x:Key="SettingsBarMoreButtonFlyoutBorderBrush" Color="#0FFFFFFF"  Core:ColorKey.Id="SettingsBarColors.MoreButtonFlyoutBorderBrush.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>



