<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="FlyoutControlForegroundBrush" Color="#FF000000" Core:ColorKey.Id="FlyoutControlColors.Foreground.Light"/>
            <SolidColorBrush x:Key="FlyoutControlBackgroundBrush" Color="#FFFFFFFF" Core:ColorKey.Id="FlyoutControlColors.Background.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="FlyoutControlForegroundBrush" Color="#FF000000" Core:ColorKey.Id="FlyoutControlColors.Foreground.Default" />
            <SolidColorBrush x:Key="FlyoutControlBackgroundBrush" Color="#FFFFFFFF" Core:ColorKey.Id="FlyoutControlColors.Background.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
</ResourceDictionary>



