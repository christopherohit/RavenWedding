<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dxcip="using:DevExpress.UI.Xaml.Controls.Internal.Pdf"
    xmlns:ControlsInternal="using:DevExpress.UI.Xaml.Controls.Internal.SettingsBar"
    xmlns:core="using:DevExpress.Core">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///DevExpress.UI.Xaml.Controls.v19.2/Themes/PdfViewerControl.Colors.xaml"/>
        <ResourceDictionary Source="ms-appx:///DevExpress.UI.Xaml.Controls.v19.2/Themes/Pdf/PdfViewThemeKeys.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <dxcip:PdfViewerControlLocalizer x:Key="PdfViewerControlLocalizer" />
    <dxcip:PdfTextSearchStateToObjectConverter x:Name="TextSearchStateToOpacityConverter" ValueTrue="1" ValueFalse="0"/>
    <dxcip:PdfTextSearchStateToBooleanConverter x:Name="TextSearchStateToBooleanConverter"/>
    <dxcip:PdfViewModeToBooleanConverter x:Key="PdfViewModeToBoolConverter"/>
    <core:BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <core:ObjectToBooleanConverter x:Key="ObjectToBoolConverter"/>
    <core:ObjectToBooleanConverter x:Key="ObjectToBoolInvertConverter" Invert="True"/>
    <core:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <core:BooleanToDoubleConverter x:Key="BoolToOpacityConverter" ValueTrue="1" ValueFalse="0" />
    <core:BooleanToDoubleConverter x:Key="BoolToOpacityInvertConverter" ValueTrue="0" ValueFalse="1" />

    <x:Double x:Key="PdfViewerOverlayDefaultOpacity">0.8</x:Double>
    <x:Double x:Key="PdfTextSearchInputActiveBackgroundOpacity">0.2</x:Double>
    <x:Double x:Key="PdfTextSearchInputInactiveBackgroundOpacity">0.15</x:Double>
</ResourceDictionary>
