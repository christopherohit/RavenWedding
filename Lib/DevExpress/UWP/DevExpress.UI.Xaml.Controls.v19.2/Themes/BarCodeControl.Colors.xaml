<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light" >
            <SolidColorBrush x:Key="BarCodeControlForegroundBrush" Color="#FF000000" Core:ColorKey.Id="BarCodeControlColors.Foreground.Light"/>
            <SolidColorBrush x:Key="BarCodeControlBackgroundBrush" Color="#FFFFFFFF" Core:ColorKey.Id="BarCodeControlColors.Background.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="BarCodeControlForegroundBrush" Color="#FF000000"  Core:ColorKey.Id="BarCodeControlColors.Foreground.Default" />
            <SolidColorBrush x:Key="BarCodeControlBackgroundBrush" Color="#FFFFFFFF"  Core:ColorKey.Id="BarCodeControlColors.Background.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>



