    <Style TargetType="controls:TransitionControl" x:Name="DefaultStyle">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="PreviousControlTemplate">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TransitionContentControl">
                    <ContentPresenter x:Name="ContentPresenter" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}">
                        <ContentPresenter.Resources>
                            <Storyboard x:Name="TransitionStory">
                                <DoubleAnimation Duration="00:00:01" Storyboard.TargetName="ContentPresenter" Storyboard.TargetProperty="Opacity" From="1" To="0" />
                            </Storyboard>
                        </ContentPresenter.Resources>
                    </ContentPresenter>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ControlTemplate">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TransitionContentControl">
                    <ContentPresenter x:Name="ContentPresenter" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}">
