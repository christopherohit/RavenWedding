        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ShapeButtonThreeStateFocusVisualWhiteStrokeThemeBrush" Color="#FFFFFFFF" Core:ColorKey.Id="ShapeButtonThreeStateColors.FocusVisualWhiteStroke.Light"/>
            <SolidColorBrush x:Key="ShapeButtonThreeStateFocusVisualBlackStrokeThemeBrush" Color="#FF000000" Core:ColorKey.Id="ShapeButtonThreeStateColors.FocusVisualBlackStroke.Light"/>

