                                        <PointerUpThemeAnimation Storyboard.TargetName="RootGrid" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="InlinePointerPressed">
                                    <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{ThemeResource HamburgerMenuButtonInlinePointerPressedBackground}" />
                                        <Setter Target="RootGrid.BorderBrush" Value="{ThemeResource HamburgerMenuButtonInlinePointerPressedBorderBrush}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerMenuButtonInlineForeground}" />
                                    </VisualState.Setters>
                                    <Storyboard>
                                        <PointerUpThemeAnimation Storyboard.TargetName="RootGrid" />
                                    </Storyboard>
                                  </VisualState>
                              <VisualState x:Name="Flyout">
                                  <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{ThemeResource HamburgerMenuButtonFlyoutBackground}" />
                                        <Setter Target="RootGrid.BorderBrush" Value="{ThemeResource HamburgerMenuButtonFlyoutBorderBrush}" />
