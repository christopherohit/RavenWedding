                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerSubMenuButtonBackgroundOpenedPressed}" />
                                        <Setter Target="PART_RootBorder.BorderBrush" Value="{ThemeResource HamburgerSubMenuButtonBorderBrushOpenedPressed}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerSubMenuButtonIconForegroundOpenedPressed}" />
                                        <Setter Target="PART_Content.Foreground" Value="{ThemeResource HamburgerSubMenuButtonContentForegroundOpenedPressed}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="OpenedDisabled">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Opacity" Value="0.3" />
                                    </VisualState.Setters>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Border x:Name="PART_IconBorder">
                                <XamlInternal:IconView x:Name="PART_Icon"
                                                       Margin="12,0"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Center" />
                            </Border>
                            <ContentPresenter x:Name="PART_Content"
                                              Foreground="{TemplateBinding Foreground}"
                                              Padding="0,0,12,0"
                                              VerticalAlignment="Center"
                                              HorizontalAlignment="Left"
                                              Grid.Column="1"
										  />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
