                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="ClosedPressed">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerSubMenuButtonBackgroundPressed}" />
                                        <Setter Target="PART_RootBorder.BorderBrush" Value="{ThemeResource HamburgerSubMenuButtonBorderBrushPressed}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerSubMenuButtonIconForegroundPressed}" />
                                        <Setter Target="PART_Content.Foreground" Value="{ThemeResource HamburgerSubMenuButtonContentForegroundPressed}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="ClosedDisabled">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Opacity" Value="0.3" />
