                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerMenuBottomBarCheckBoxBackgroundPressed}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerMenuBottomBarCheckBoxIconFillPressed}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="UncheckedDisabled">
