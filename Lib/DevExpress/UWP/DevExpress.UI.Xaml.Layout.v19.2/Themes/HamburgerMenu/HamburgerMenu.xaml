                                </VisualState>
                                <VisualState x:Name="InlinePointerOver">
                                    <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{ThemeResource HamburgerMenuButtonInlinePointerOverBackground}" />
                                        <Setter Target="RootGrid.BorderBrush" Value="{ThemeResource HamburgerMenuButtonInlinePointerOverBorderBrush}" />
