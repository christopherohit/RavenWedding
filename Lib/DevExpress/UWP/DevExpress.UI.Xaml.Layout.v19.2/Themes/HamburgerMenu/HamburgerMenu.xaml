    <!--#region Custom Window Title Settings-->
    <DataTemplate x:Key="WindowTitleSettings">
        <Xaml:TitleSettings Background="{ThemeResource TitleBarBackground}"
                        Foreground="{ThemeResource TitleBarForeground}"
                        
                        InactiveBackground="{ThemeResource TitleBarInactiveBackground}"
                        InactiveForeground="{ThemeResource TitleBarInactiveForeground}"
                        
                        ButtonBackground="{ThemeResource TitleBarButtonBackground}"
                        ButtonForeground="{ThemeResource TitleBarButtonForeground}"
                        
                        ButtonInactiveBackground="{ThemeResource TitleBarButtonInactiveBackground}"
                        ButtonInactiveForeground="{ThemeResource TitleBarButtonInactiveForeground}"
                        
                        ButtonHoverBackground="{ThemeResource TitleBarButtonHoverBackground}"
