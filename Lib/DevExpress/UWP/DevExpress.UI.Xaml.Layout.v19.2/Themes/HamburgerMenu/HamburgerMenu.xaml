                            <VisualStateGroup x:Name="CombinedStates">
                                <VisualState x:Name="ClosedNormal" />
                                <VisualState x:Name="ClosedPointerOver">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerSubMenuButtonBackgroundHover}" />
                                        <Setter Target="PART_RootBorder.BorderBrush" Value="{ThemeResource HamburgerSubMenuButtonBorderBrushHover}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerSubMenuButtonIconForegroundHover}" />
