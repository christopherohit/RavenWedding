                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellForegroundAnotherViewSelectedBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ForegroundFromAnotherView">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Foreground" Storyboard.TargetName="Presenter">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellForegroundAnotherViewBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ForegroundHover">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Foreground" Storyboard.TargetName="Presenter">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellForegroundHoverBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ForegroundSelected">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Foreground" Storyboard.TargetName="Presenter">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellForegroundSelectedBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ForegroundHoliday">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Foreground" Storyboard.TargetName="Presenter">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellForegroundHolidayBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ForegroundNormal" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <dxe:CellPresenter x:Name="Presenter" ActualContainerSize="{Binding ActualSize, RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                                                   Foreground="{ThemeResource HamburgerMenuDateNavigatorCellForegroundBrush}" />
                        <Border BorderBrush="{ThemeResource HamburgerMenuDateNavigatorCellCaretBorderBrush}"
                                                        Background="{x:Null}"
                                                        BorderThickness="0"
                                                        x:Name="PART_Caret" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
