                                <VisualState x:Name="CheckedNormal">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerMenuBottomBarCheckBoxBackgroundChecked}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerMenuBottomBarCheckBoxIconFillChecked}" />
