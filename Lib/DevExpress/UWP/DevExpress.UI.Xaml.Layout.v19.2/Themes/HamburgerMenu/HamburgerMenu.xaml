                                        <Setter Target="PART_RootBorder.BorderBrush" Value="{ThemeResource HamburgerSubMenuButtonBorderBrushOpened}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerSubMenuButtonIconForegroundOpened}" />
                                        <Setter Target="PART_Content.Foreground" Value="{ThemeResource HamburgerSubMenuButtonContentForegroundOpened}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="OpenedPointerOver">
                                    <VisualState.Setters>
                                        <Setter Target="PART_RootBorder.Background" Value="{ThemeResource HamburgerSubMenuButtonBackgroundOpenedHover}" />
                                        <Setter Target="PART_RootBorder.BorderBrush" Value="{ThemeResource HamburgerSubMenuButtonBorderBrushOpenedHover}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerSubMenuButtonIconForegroundOpenedHover}" />
                                        <Setter Target="PART_Content.Foreground" Value="{ThemeResource HamburgerSubMenuButtonContentForegroundOpenedHover}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="OpenedPressed">
