                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="BlackoutHover">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Background" Storyboard.TargetName="PART_Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellBackgroundBlackoutBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="Selected">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Background" Storyboard.TargetName="PART_Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource HamburgerMenuDateNavigatorCellBackgroundSelectedBrush}" />
