                                        <Setter Target="RootGrid.BorderBrush" Value="{ThemeResource HamburgerMenuButtonFlyoutPointerOverBorderBrush}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerMenuButtonFlyoutForeground}" />
                                  </VisualState.Setters>
                                  <Storyboard>
                                        <PointerUpThemeAnimation Storyboard.TargetName="RootGrid" />
                                  </Storyboard>
                              </VisualState>
                              <VisualState x:Name="FlyoutPointerPressed">
                                    <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{ThemeResource HamburgerMenuButtonFlyoutPointerPressedBackground}" />
                                        <Setter Target="RootGrid.BorderBrush" Value="{ThemeResource HamburgerMenuButtonFlyoutPointerPressedBorderBrush}" />
                                        <Setter Target="PART_Icon.Foreground" Value="{ThemeResource HamburgerMenuButtonFlyoutForeground}" />
