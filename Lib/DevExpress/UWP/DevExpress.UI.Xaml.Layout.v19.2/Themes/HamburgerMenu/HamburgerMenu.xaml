                <Core:BindableSetterCollection>
                    <Core:BindableSetter Property="WindowTitleSettings" 
                                         Binding="{Binding Source={StaticResource WindowTitleSettings}, Converter={StaticResource templateConverter}, Mode=OneTime}" />
                </Core:BindableSetterCollection>
            </Setter.Value>
        </Setter>
        <Setter Property="AllowCustomizingWindowTitle" Value="True" />
        <Setter Property="MobileStatusBarBackground" Value="{ThemeResource MobileStatusBarBackground}" />
        <Setter Property="MobileStatusBarForeground" Value="{ThemeResource MobileStatusBarForeground}" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:TileBarFrame">
                    <Grid x:Name="PART_RootGrid"
                          >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="TileBarPlacementStates">
                                <VisualState x:Name="Top">
                                    <VisualState.Setters>
                                        <Setter Target="PART_Menu.VerticalAlignment" Value="Top" />
                                        <Setter Target="PART_Menu.(Grid.Column)" Value="1" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Bottom">
                                    <VisualState.Setters>
                                        <Setter Target="PART_Menu.VerticalAlignment" Value="Bottom" />
                                        <Setter Target="PART_Menu.(Grid.Row)" Value="2" />
                                        <Setter Target="PART_Menu.(Grid.Column)" Value="1" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Left">
                                    <VisualState.Setters>
                                        <Setter Target="PART_Menu.HorizontalAlignment" Value="Left" />
                                        <Setter Target="PART_Menu.(Grid.Row)" Value="1" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Right">
                                    <VisualState.Setters>
                                        <Setter Target="PART_Menu.HorizontalAlignment" Value="Right" />
                                        <Setter Target="PART_Menu.(Grid.Row)" Value="1" />
                                        <Setter Target="PART_Menu.(Grid.Column)" Value="2" />
                                    </VisualState.Setters>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid x:Name="PART_TitlePanel" Visibility="Collapsed" Canvas.ZIndex="2" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <internal:WindowTitleBackButton x:Name="PART_WindowBackButton" Visibility="Collapsed" />
                            <Border x:Name="PART_TitleBar" Grid.Column="1" Background="Transparent">
                                <ContentControl x:Name="PART_TitleText" Padding="12, 9,12,8" FontSize="12" FontFamily="Segoe UI Light" HorizontalContentAlignment="Left" />
                            </Border>
                        </Grid>
                        <Grid x:Name="PART_Container"
                              Grid.Row="1"
                              Background="{TemplateBinding Background}"
                              BorderBrush="{TemplateBinding BorderBrush}"
                              BorderThickness="{TemplateBinding BorderThickness}"
                              Padding="{TemplateBinding Padding}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <ContentPresenter x:Name="PART_Menu"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Stretch"
                                              />
                            <ContentPresenter Grid.Row="1"
                                              Grid.Column="1"
                                              ContentTemplate="{TemplateBinding ContentTemplate}"
                                              ContentTransitions="{TemplateBinding ContentTransitions}"
                                              Content="{TemplateBinding Content}"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Stretch"
                                              HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                              VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" />
                        </Grid>
                        <Grid x:Name="PART_SplashScreen" Visibility="Collapsed" x:DeferLoadStrategy="Lazy" Grid.Row="1"
                              />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
