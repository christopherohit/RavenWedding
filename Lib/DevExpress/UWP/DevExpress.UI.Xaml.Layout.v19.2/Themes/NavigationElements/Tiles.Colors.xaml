<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="TileCheckFocusVisualWhiteStrokeThemeBrush" Color="#FFFFFFFF" Core:ColorKey.Id="TileCheckColors.FocusVisualWhiteStroke.Default" />
            <SolidColorBrush x:Key="TileCheckFocusVisualBlackStrokeThemeBrush" Color="#FF000000" Core:ColorKey.Id="TileCheckColors.FocusVisualBlackStroke.Default" />
            <SolidColorBrush x:Key="TileCheckDisabledForegroundThemeBrush" Color="#66000000" Core:ColorKey.Id="TileCheckColors.ForegroundDisabled.Default" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="TileCheckFocusVisualWhiteStrokeThemeBrush" Color="#FFFFFFFF" Core:ColorKey.Id="TileCheckColors.FocusVisualWhiteStroke.Light"/>
            <SolidColorBrush x:Key="TileCheckFocusVisualBlackStrokeThemeBrush" Color="#FF000000" Core:ColorKey.Id="TileCheckColors.FocusVisualBlackStroke.Light"/>
            <SolidColorBrush x:Key="TileCheckDisabledForegroundThemeBrush" Color="#66000000" Core:ColorKey.Id="TileCheckColors.ForegroundDisabled.Light"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>



