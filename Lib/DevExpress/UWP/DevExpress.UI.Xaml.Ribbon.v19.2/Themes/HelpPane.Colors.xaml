    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="RibbonHelpPaneViewSeparatorBrush" Color="#66FFFFFF" Core:ColorKey.Id="RibbonControlColors.HelpPaneSeparator.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="RibbonHelpPaneViewSeparatorBrush" Color="#66FFFFFF" Core:ColorKey.Id="RibbonControlColors.HelpPaneSeparator.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>


