<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileForegroundBrush" Color="#AF000000" Core:ColorKey.Id="ToolbarFlyoutMobileColors.Foreground.Light"/>
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileBackgroundBrush" Color="#FFFFFFFF" Core:ColorKey.Id="ToolbarFlyoutMobileColors.Background.Light"/>
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileBorderBrush" Color="#FFD2D2D2" Core:ColorKey.Id="ToolbarFlyoutMobileColors.BorderBrush.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileForegroundBrush" Color="#AF000000" Core:ColorKey.Id="ToolbarFlyoutMobileColors.Foreground.Default" />
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileBackgroundBrush" Color="#FF424242" Core:ColorKey.Id="ToolbarFlyoutMobileColors.Background.Default" />
            <SolidColorBrush x:Key="ToolbarStackedFlyoutControlMobileBorderBrush" Color="#FF737373" Core:ColorKey.Id="ToolbarFlyoutMobileColors.BorderBrush.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>




