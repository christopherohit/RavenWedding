<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:DevExpress.Core">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ChartSeriesTooltipForegroundBrush" Color="#FFFFFFFF" Core:ColorKey.Id="ChartColors.SeriesTooltipForeground.Light"/>
            <SolidColorBrush x:Key="ChartsToolTipBackgroundBrush" Color="Black" Core:ColorKey.Id="ChartColors.ToolTipBackground.Light"/>
            <SolidColorBrush x:Key="ChartsToolTipForegroundBrush" Color="White" Core:ColorKey.Id="ChartColors.ToolTipForeground.Light"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ChartSeriesTooltipForegroundBrush" Color="#FFFFFFFF"  Core:ColorKey.Id="ChartColors.SeriesTooltipForeground.Default" />
            <SolidColorBrush x:Key="ChartsToolTipBackgroundBrush" Color="Black"  Core:ColorKey.Id="ChartColors.ToolTipBackground.Default" />
            <SolidColorBrush x:Key="ChartsToolTipForegroundBrush" Color="White"  Core:ColorKey.Id="ChartColors.ToolTipForeground.Default" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>




